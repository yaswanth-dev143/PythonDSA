    "# 4. Median of Two Sorted Arrays\n",
    "def medianofsortedarr(num1 , num2):\n",
    "    num = nums1 + nums2\n",
    "    num.sort()\n",
    "    res = num[0] + num[len(num)-1]\n",
    "    return (float(res)) / 2\n",
    "\n",
    "nums1 = [1,2]\n",
    "nums2 = [3,4]\n",
    "medianofsortedarr(nums1,nums2)"
   ]
